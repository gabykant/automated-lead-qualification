      "name": "Normalize Input",
      "alwaysOutputData": true
    },
    {
      "parameters": {
        "jsCode": "const input = $input.item.json;\n\nlet score = 0;\n\n// 1️⃣ Company size\nif (input.company_size === \"1-10\") score += 10;\nif (input.company_size === \"10-50\") score += 20;\nif (input.company_size === \"50+\") score += 30;\n\n// 2️⃣ Budget\nif (input.budget === \"$1,000 - $5,000\") score += 40;\nelse if (input.budget === \"$500 - $1,000\") score += 20;\nelse if (input.budget === \"0 - $500\") score += 5;\n\nif (input.need && input.need.toLowerCase().includes(\"automation\")) {\n  score += 20;\n}\n\nif (input.urgent.toLowerCase() === \"urgent\") score += 10;\n\n// 5️⃣ Classification\nlet lead_status = \"COLD\";\nif (score >= 70) lead_status = \"HOT\";\nelse if (score >= 40) lead_status = \"WARM\";\n\n// Output enrichi\nreturn {\n  json: {\n    ...input,\n    score,\n    lead_status\n  }\n};\n"
      },
