    {
      "parameters": {
        "jsCode": "const input = $input.item.json.body;\n\n// Mapping simple et explicite\nreturn {\n  json: {\n    name: input.fullname || \"\",\n    email: input.email || \"\",\n    company_size: input.company_size || \"\",\n    budget: input.estimated_budget || \"\",\n    need: input.need || \"\",\n    urgent: input.urgent || \"\"\n  }\n};\n"
      },
